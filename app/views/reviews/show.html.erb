<h2><%= @doctor.name%>'s Review</h2>
<% @doctor.reviews.each do |review| %>
    <%= review.content%><br><br>
    <%= link_to review.content, review_path(review)%>
    <%= link_to "Edit", edit_doctor_review_path(@doctor) %><br>
    <%= link_to "Delete", doctor_review_path(@doctor), method: :delete%>
<%end%>