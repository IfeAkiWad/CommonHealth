<%= form_for @patient do |f| %>
  Name: <%= f.text_field :name %><br><br>
  Age: <%= f.text_field :age %><br><br>
  Location: <%= f.text_field :location %><br><br>
  Username: <%= f.text_field :username %><br><br>
  Email: <%= f.text_field :email %><br><br>
  Password: <%= f.text_field :password %><br><br>
  <%= f.submit %>
<% end %>